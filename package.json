    "convert": "node convert.js",
    "clear-lib": "rimraf lib",
    "ngc-build": "ngc -p ./src/tsconfig.ngc.json",
    "dist": "npm run clear-lib && npm run ngc-build"
  },
  "typings": "./index.d.ts",
  "main": "./index.js",
  "bin": {
    "angular2-image-gallery": "./convert.js"
  },
  "private": false,
